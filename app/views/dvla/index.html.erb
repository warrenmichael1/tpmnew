<table class="ui celled table">
  <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Date Of Birth</th>
        <th>ID Number</th>
    </tr>
  </thead>
  <tbody>
    <% @dvla_users.each do |user| %>
        <tr>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= user.date_of_birth %></td>
            <td></td>
        </tr>
    <% end %> 
  </tbody>
</table>